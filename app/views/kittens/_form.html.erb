<%= form_with model: @kitten do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name, placeholder: "name" %>

    <br>
    <%= f.label :age %>
    <%= f.number_field :age, in: 1..20 %>

    <br>
    <%= f.label :cuteness %>
    <%= f.number_field :age, in: 1..10 %>
    
    <br>
    <%= f.label :softness %>
    <%= f.number_field :age, in: 1..10 %>

    <%= f.submit "Meow!" %>
<% end %>